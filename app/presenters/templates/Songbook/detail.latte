{block content}
<header id="banner">
    <h1 n:block=title class="title title-song">{$title}</h1>
</header>

<section class="list">
    <ul>
        <li n:foreach="$songs as $song">
            <a n:href="Song:detail $song->guid">{$song->title}, {$song->interpreter}</a>
            <a class="icon icon-edit" n:href="Song:edit $song->guid">{include '../components/shape.latte' name => 'pen'}</a>
        </li>
    </ul>
</section>

